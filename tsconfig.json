{
  "compilerOptions": {
    "lib": ["es2017"],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "es2017",
    "outDir": "lib",
    "baseUrl": "./",
    "rootDir": "./",
    "paths": {
      "types/*": ["src/types/*"],
      "constants/*": ["src/constants/*"],
      "helpers/*": ["src/helpers/*"]
    }
  },
  "exclude": ["node_modules"]
}
